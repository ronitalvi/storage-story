<h1>storages</h1>
<div class="container">
  <div class="row">
    <div class="col-sm-4 col-sm-offset-2">
      <table>
        <thead>
          <tr>
            <th>Adress</th>
            <th>price</th>
            <th>desc.</th>
            <th>name</th>
            <th>photo</th>
            <th>sqm</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @storages.each do |storage| %>
          <tr>
            <td><%= storage.address %></td>
            <td><%= storage.price %></td>
            <td><%= storage.description %></td>
            <td><%= storage.name %></td>
            <td><%= storage.photo %></td>
            <td><%= storage.sqm %></td>


            <td><%= link_to 'Show', storage %></td>


            <td><%= link_to 'Edit', edit_storage_path(storage) %></td>


            <td><%= link_to 'Destroy', storage_path(storage), method: :delete, data: { confirm: 'Are you sure?' } %></td>

          </tr>
          <% end -%>
        </tbody>
      </table>
      <br>

      <%= link_to 'New storage', new_storage_path %>

    </div>
  </div>
</div>
